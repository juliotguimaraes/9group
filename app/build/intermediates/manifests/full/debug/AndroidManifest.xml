<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ninegroup.ninegame"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen" >
        <activity
            android:name="ninegroup.ninegame.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ninegroup.ninegame.QuestionActivity"
            android:label="@string/title_activity_question" >
        </activity>
        <activity
            android:name="ninegroup.ninegame.CreditosActivity"
            android:label="@string/title_activity_creditos"
            android:parentActivityName="ninegroup.ninegame.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ninegroup.ninegame.MainActivity" />
        </activity>
        <activity
            android:name="ninegroup.ninegame.RankingActivity"
            android:label="@string/title_activity_ranking"
            android:parentActivityName="ninegroup.ninegame.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="ninegroup.ninegame.MainActivity" />
        </activity>
    </application>

</manifest>