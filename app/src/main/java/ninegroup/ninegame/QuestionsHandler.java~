    }

    // level: numero de 1 a 5
    // question: numero de 1 a 20
    public Question getSpecificQuestionFromSpecificNormalLevel(int level, int question){
        if(question < 1 || question > 20)
            return null;
        switch (level) {
            case 1:
                return nql1.get(question-1);
            case 2:
                return nql2.get(question-1);
            case 3:
                return nql3.get(question-1);
            case 4:
                return nql4.get(question-1);
            case 5:
                return nql5.get(question-1);
            default:
                return null;
        }
    }
    // level: numero de 1 a 5
    public Question getRandomQuestionFromSpecificNormalLevel(int level){
        int max = 20,min = 1;
        Random rand = new Random();
        int rQuestion = (rand.nextInt((max - min) + 1) + min);
        switch (level){
            case 1:
                return nql1.get(rQuestion-1);
            case 2:
                return nql2.get(rQuestion-1);
            case 3:
                return nql3.get(rQuestion-1);
            case 4:
                return nql4.get(rQuestion-1);
            case 5:
                return nql5.get(rQuestion-1);
            default:
                return null;
        }
    }
    public Question getRandomQuestionFromRandomNormalLevel() {
        int maxQ = 20,minQ = 1;
        Random rand = new Random();
        int rQuestion = rand.nextInt((maxQ - minQ) + 1) + minQ;
        int maxL = 5, minL = 1;
        int rLevel = rand.nextInt((maxL - minL) + 1) + minL;
        switch (rLevel){
            case 1:
                return nql1.get(rQuestion-1);
            case 2:
                return nql2.get(rQuestion-1);
            case 3:
                return nql3.get(rQuestion-1);
            case 4:
                return nql4.get(rQuestion-1);
            case 5:
                return nql5.get(rQuestion-1);
            default:
                return nql1.get(0);
        }
    }


    // changeLevel: numero de 1 a 5
    // question: numero de 1 a 10
    public Question getSpecificQuestionFromSpecificChangeLevel(int changeLevel, int question){
        if(question < 1 || question > 10)
            return null;
        switch(changeLevel){
            case 1:
                return qcl1.get(question-1);
            case 2:
                return qcl2.get(question-1);
            case 3:
                return qcl3.get(question-1);
            case 4:
                return qcl4.get(question-1);
            case 5:
                return qcl5.get(question-1);
            default:
                return null;
        }
    }
    // changeLevel: numero de 1 a 5
    public Question getRandomQuestionFromChangeLevel(int changeLevel){
        int maxQ = 10,minQ = 1;
        Random rand = new Random();
        int rQuestion = rand.nextInt((maxQ - minQ) + 1) + minQ;
        switch(changeLevel){
            case 1:
                return qcl1.get(rQuestion-1);
            case 2:
                return qcl2.get(rQuestion-1);
            case 3:
                return qcl3.get(rQuestion-1);
            case 4:
                return qcl4.get(rQuestion-1);
            case 5:
                return qcl5.get(rQuestion-1);
            default:
                return null;
        }
    }
    public Question getRandomQuestionFromRandomChangeLevel() {
        int maxQ = 10,minQ = 1;
        Random rand = new Random();
        int rQuestion = rand.nextInt((maxQ - minQ) + 1) + minQ;
        int maxL = 5, minL = 1;
        int rLevel = rand.nextInt((maxL - minL) + 1) + minL;
        switch (rLevel){
            case 1:
                return qcl1.get(rQuestion-1);
            case 2:
                return qcl2.get(rQuestion-1);
            case 3:
                return qcl3.get(rQuestion-1);
            case 4:
                return qcl4.get(rQuestion-1);
            case 5:
                return qcl5.get(rQuestion-1);
            default:
                return qcl1.get(0);
        }
    }

